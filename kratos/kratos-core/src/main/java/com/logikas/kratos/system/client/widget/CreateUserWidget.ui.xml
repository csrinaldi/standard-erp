<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:e="urn:import:com.google.gwt.editor.ui.client">

	<ui:style>
		
	</ui:style>
	<g:VerticalPanel>
		<g:Image width="100px" height="100px" ui:field="avatarView"></g:Image>

		<g:FormPanel ui:field="formPanel">
			<g:FileUpload name="avatar" ui:field="avatarUploader"></g:FileUpload>
		</g:FormPanel>

		<e:ValueBoxEditorDecorator ui:field="name">
			<e:valuebox>
				<g:TextBox></g:TextBox>
			</e:valuebox>
		</e:ValueBoxEditorDecorator>
		<g:Button ui:field="save">save</g:Button>
	</g:VerticalPanel>
</ui:UiBinder> 
